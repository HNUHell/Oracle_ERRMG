{
    "title": "数据库异常信息",
    "author": "helili",
    "language": "zh-hans",
    "root": ".",
    "plugins": [
        "expandable-chapters-small",
        "anchors",
        "anchor-navigation-ex",
        "edit-link",
        "tbfed-pagefooter",
        "splitter",
        "donate",
        "github",
        "github-buttons",
        "sitemap",
        "-lunr", "-search", "search-plus",
        "-highlight", "prism", "prism-themes"
    ],
    "pluginsConfig": {
        "expandable-chapters-small":{},
        "anchor-navigation-ex": {
			"multipleH1" : false,
			"showLevel" : false
        },
        "edit-link": {
            "base": "https://github.com/HNUHell/Oracle_ERRMG/tree/gitbook",
            "label": "编辑本页"
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy he.lili 2017",
            "modify_label": "最新修订日期：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "donate": {
            "wechat": "/oracle_errmg/content/images/wechat.png",
            "alipay": "/oracle_errmg/content/images/alipay.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "github": {
            "url": "https://github.com/HNUHell/Oracle_ERRMG/tree/gitbook"
        },
        "github-buttons": {
            "repo": "HNUHell/Oracle_ERRMG",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "sitemap": {
            "hostname": "https://hnuhell.gitbooks.io/oracle_errmg/"
        },
        "prism": {
            "css": [
              "prism-themes/themes/prism-duotone-earth.css"
            ]
        },
		"fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        }
